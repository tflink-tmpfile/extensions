{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tmpfile-link",
  "title": "tmpfile.link",
  "description": "Upload clipboard text to tmpfile.link anonymously and get a shareable link.",
  "icon": "icon.png",
  "author": "antigravity",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "upload",
      "title": "Upload Clipboard to tmpfile.link",
      "subtitle": "tmpfile.link",
      "description": "Uploads the current clipboard text to tmpfile.link and copies the URL.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@raycast/utils": "^1.11.0",
    "@types/node": "20.8.10",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "dev": "ray dev",
    "build": "ray build -e dist -o dist",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}